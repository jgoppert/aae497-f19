<?xml version='1.0'?>
<sdf version='1.6'>
  <model name="rocket">

    <link name='body'>
      <inertial>
        <pose>1.0 0 0 0 1.57 0</pose>
      </inertial>
      <collision name='body_collision'>
        <pose>0 0 0 0 1.57 0</pose>
        <geometry>
          <mesh>
            <uri>model://rocket/meshes/stl/body.stl</uri>
          </mesh>
        </geometry>
      </collision>
      <visual name='body_visual'>
        <pose>0 0 0 0 1.57 0</pose>
        <geometry>
          <mesh>
            <uri>model://rocket/meshes/stl/body.stl</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Grey</name>
          </script>
        </material>
      </visual>
      <collision name='nose_collision'>
        <pose>0 0 0 0 1.57 0</pose>
        <geometry>
          <mesh>
            <uri>model://rocket/meshes/stl/nose_cone.stl</uri>
          </mesh>
        </geometry>
      </collision>
      <visual name='nose_visual'>
        <pose>0 0 0 0 1.57 0</pose>
        <geometry>
          <mesh>
            <uri>model://rocket/meshes/stl/nose_cone.stl</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/White</name>
          </script>
        </material>
      </visual>
    </link>

    <link name='fin0'>
      <pose>0 0 0.1 0 0 0</pose>
      <inertial>
        <mass>1e-5</mass>
        <inertia>
          <ixx>1e-5</ixx>
          <iyy>1e-5</iyy>
          <izz>1e-5</izz>
        </inertia>
      </inertial>
      <visual name='visual'>
        <geometry>
          <mesh>
            <uri>model://rocket/meshes/stl/fin.stl</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Red</name>
          </script>
        </material>
      </visual>
    </link>

    <joint name="fin0_joint" type="revolute">
      <pose>0.3 0 0 0 0 0</pose>
      <parent>body</parent>
      <child>fin0</child>
      <axis>
        <xyz>0 0 -1</xyz>
      </axis>
    </joint>

    <link name='fin1'>
      <pose>0 0.1 0 -1.57 0 0</pose>
       <inertial>
        <mass>1e-5</mass>
        <inertia>
          <ixx>1e-5</ixx>
          <iyy>1e-5</iyy>
          <izz>1e-5</izz>
        </inertia>
      </inertial>
      <visual name='visual'>
        <geometry>
          <mesh>
            <uri>model://rocket/meshes/stl/fin.stl</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Green</name>
          </script>
        </material>
      </visual>
    </link>

    <joint name="fin1_joint" type="revolute">
      <pose>0.3 0.0 0 0 0 0</pose>
      <parent>body</parent>
      <child>fin1</child>
      <axis>
        <xyz>0 0 -1</xyz>
      </axis>
    </joint>

    <link name='fin2'>
      <pose>0 0 -0.1 3.14 0 0</pose>
      <inertial>
        <mass>1e-5</mass>
        <inertia>
          <ixx>1e-5</ixx>
          <iyy>1e-5</iyy>
          <izz>1e-5</izz>
        </inertia>
      </inertial>
      <visual name='visual'>
        <geometry>
          <mesh>
            <uri>model://rocket/meshes/stl/fin.stl</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Blue</name>
          </script>
        </material>
      </visual>
    </link>

    <joint name="fin2_joint" type="revolute">
      <pose>0.3 0.0 0 0 0 0</pose>
      <parent>body</parent>
      <child>fin2</child>
      <axis>
        <xyz>0 0 -1</xyz>
      </axis>
    </joint>

    <link name='fin3'>
      <pose>0 -0.1 0 1.57 0 0</pose>
      <inertial>
        <mass>1e-5</mass>
        <inertia>
          <ixx>1e-5</ixx>
          <iyy>1e-5</iyy>
          <izz>1e-5</izz>
        </inertia>
      </inertial>
      <visual name='visual'>
        <geometry>
          <mesh>
            <uri>model://rocket/meshes/stl/fin.stl</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/White</name>
          </script>
        </material>
      </visual>
    </link>

    <joint name="fin3_joint" type="revolute">
      <pose>0.3 0.0 0 0 0 0</pose>
      <parent>body</parent>
      <child>fin3</child>
      <axis>
        <xyz>0 0 -1</xyz>
      </axis>
    </joint>

    <plugin name="rocket_plugin" filename="libRocketPlugin.so">
      <body>body</body>
      <fin0>fin0_joint</fin0>
      <fin1>fin1_joint</fin1>
      <fin2>fin2_joint</fin2>
      <fin3>fin3_joint</fin3>
    </plugin>

  </model>
</sdf>

